<eainfo>
  <detailed>
    <enttyp>
      <enttypl>
Election Precincts</enttypl>
    </enttyp>
    <attr>
      <attrlabl>GEOID</attrlabl>
      <attrdef>unique identifier for the precinct, formed from the five-digit county FIPS code followed by the precinct name/ID</attrdef>
      <attrdefs>NYT</attrdefs>
    </attr>
    <attr>
      <attrlabl>votes_dem</attrlabl>
      <attrdef>votes received by Joseph Biden</attrdef>
      <attrdefs>NYT</attrdefs>
    </attr>
    <attr>
      <attrlabl>votes_rep</attrlabl>
      <attrdef>votes received by Donald Trump</attrdef>
      <attrdefs>NYT</attrdefs>
    </attr>
    <attr>
      <attrlabl>votes_total</attrlabl>
      <attrdef>total votes in the precinct, including for third-party candidates and write-ins</attrdef>
      <attrdefs>NYT</attrdefs>
    </attr>
    <attr>
      <attrlabl>votes_per_sqkm</attrlabl>
      <attrdef>(votes_dem - votes_rep) / (votes_total), rounded to one decimal place </attrdef>
      <attrdefs>NYT</attrdefs>
    </attr>
    <attr>
      <attrlabl>pct_dem_lead</attrlabl>
      <attrdef>total votes divided by the area of the precinct, rounded to one decimal place</attrdef>
      <attrdefs>NYT</attrdefs>
    </attr>
  </detailed>
</eainfo>

